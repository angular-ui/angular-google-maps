les: [\".ignore\", \".gitignore\"]\n       })\n  .on(\"child\", function (c) {\n    console.error(c.path.substr(c.root.path.length + 1))\n  })\n  .pipe(tar.Pack())\n  .pipe(fs.createWriteStream(\"foo.tar\"))\n```\n\nThis will tar up the files in __dirname into `foo.tar`, ignoring\nanything matched by the globs in any .iginore or .gitignore file.\n","maintainers":[{"name":"isaacs","email":"i@izs.me"}],"time":{"0.0.1":"2012-03-22T06:05:23.323Z","0.0.2":"2012-03-26T21:37:33.128Z","0.0.3":"2012-03-27T06:38:38.007Z","0.0.4":"2012-03-27T20:17:58.489Z","0.0.5":"2012-03-27T20:45:31.749Z","0.0.6":"2013-01-17T17:09:08.879Z","0.0.7":"2013-05-29T00:35:12.452Z"},"author":{"name":"Isaac Z. Schlueter","email":"i@izs.me","url":"http://blog.izs.me/"},"repository":{"type":"git","url":"git://github.com/isaacs/fstream-ignore.git"},"_attachments":{"fstream-ignore-0.0.7.tgz":{"content_type":"application/octet-stream","revpos":18,"digest":"md5-472YdV9vIhKposfhKrkAHA==","length":6014,"stub":true},"fstream-ignore-0.0.6.tgz":{"content_type":"application/octet-stream","revpos":17,"digest":"md5-iVW9eyx8paFGyU9edh96gg==","length":6012,"stub":true},"fstream-ignore-0.0.5.tgz":{"content_type":"application/octet-stream","revpos":15,"digest":"md5-dF35HhX5pkqRLCzWcMoFhA==","length":5230,"stub":true},"fstream-ignore-0.0.4.tgz":{"content_type":"application/octet-stream","revpos":13,"digest":"md5-eMtONwcTVS1AEn6XhDj6Vg==","length":5222,"stub":true},"fstream-ignore-0.0.3.tgz":{"content_type":"application/octet-stream","revpos":11,"digest":"md5-67YEmwvZ7ycNT+n0dfhHGA==","length":5173,"stub":true},"fstream-ignore-0.0.2.tgz":{"content_type":"application/octet-stream","revpos":9,"digest":"md5-iCeLqDzbL1sZndbWCS0Hxw==","length":5263,"stub":true},"fstream-ignore-0.0.1.tgz":{"content_type":"application/octet-stream","revpos":3,"digest":"md5-2I9n51uAnsK/Zb8gW5yIKA==","length":4329,"stub":true}},"_etag":"\"DO7YBJAXMWB55Z408MZ4RZIRB\""}